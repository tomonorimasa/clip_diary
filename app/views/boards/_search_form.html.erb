<%= search_form_for q, url: url do |f| %>
  <div class='input-group mb-3'>
    <%= f.search_field :title_or_body_cont, class: 'form-control me-2' , id: 'search_field_of_board' , placeholder: 'タイトル or 本文' %>
    <%= f.search_field :user_nickname_cont, class: 'form-control me-2' , id: 'search_field_of_user' ,placeholder: 'ユーザー' %>
    <div class="input-group-append">
      <%= f.submit '検索' , class: 'btn btn-outline-success' %>
    </div>
  </div>
<% end %>